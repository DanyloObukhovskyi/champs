<div class="component ratings-teams">
  <div class="header">
    <div class="round">
      <div><img src="/images/lamp.svg" alt="lamp" /></div>
    </div>
    <div class="title">
      Рейтинг команд
    </div>
  </div>
  <div class="table">
    {% set i = 1 %}
    {% for item in ratingCommands %}
    <div class="table-row">
      <div>
        <span class="num">{{i}}.</span>
        <img src="{{item.image}}" />
        <label>{{item.title}}</label>
      </div>
      <div>
        {{ item.rating }}
      </div>
    </div>
    {% set i = i + 1 %}
    {% endfor %}
  </div>
</div>
