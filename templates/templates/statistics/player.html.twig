<div class="component player">
  <div class="header">
    <div class="round">
      <div><img src="/images/lamp.svg" alt="lamp" /></div>
    </div>
    <div class="title">
      Игрок недели
    </div>
  </div>
  {% for item in playerWeek %}
  <div class="content">
    <div class="image">
      <img src="{{item.image}}" />
    </div>
    <div class="data">
      <div class="nik">
        <img src="/images/temp/flag-russia.png" alt="flag-russia">
        {{item.nickname}}
      </div>
      <div class="name">
        {{item.fullname}}
      </div>
      <div class="score">
        {{item.rate}}/раунд
      </div>
      <div class="bonus">
        {{item.skill}}
      </div>
      <div class="tab">
        <div>рейтинг</div>
        <div>{{item.rating}}</div>
      </div>
      <div class="tab">
        <div>убийств за раунд</div>
        <div>{{item.killRound}}</div>
      </div>
      <div class="tab">
        <div>% без смертей</div>
        <div>{{item.percentLife}}</div>
      </div>
      <div class="tab">
        <div>влияние</div>
        <div>{{item.effect}}</div>
      </div>
      <div class="tab">
        <div>средний урон</div>
        <div>{{item.damage}}</div>
      </div>
    </div>
  </div>
  {% endfor %}
</div>
