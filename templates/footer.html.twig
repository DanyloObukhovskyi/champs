<script>
    var cookie = new Vue({
        el: '#cookie',
        data: function () {
            return {
                start: true,
                modal: false,
            };
        },
        methods: {
            setCookie() {
                this.start = false;
                axios.get('/ru/setcookies').then();
            },
            up() {
                let t;
                let top = Math.max(document.body.scrollTop,document.documentElement.scrollTop);
                if(top > 0) {
                    window.scrollBy(0,-100);
                    t = setTimeout(cookie.up, 20);
                } else clearTimeout(t);
                return false;
            }
        },
    });
</script>